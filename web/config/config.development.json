{
  "server": {
    "host": "127.0.0.1",
    "port": 3000
  },
  "security": {
    "csrf": true
  },
  "api": {
    "enabled": false
  },
  "engines": {
    "dust": {
      "whitespace": true
    }
  },
  "headers": {
    "useCompression": true,
    "cacheControl": {
      "text/html": "no-cache"
    }
  },
  "globalEvents": [
    "i18n"
  ],
  "globalPostProcessors": [
    "minify-html"
  ],
  "rewrites": {
    "path": "./workspace/routes/rewrites.txt"
  },
  "cluster": false,
  "allowJsonView": true,
  "debug": true
}

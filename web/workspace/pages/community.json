    "pages",
    "web-services",
    "categories",
    "category-articles",
    "tutorials-articles",
    "updates-articles",
    "knowledge-articles"
  ],
  "events": [
    "metatags"
  ],
  "routes": [
    {
      "path": "/:lang/community"
    },
    {
      "path": "/:lang/:category",

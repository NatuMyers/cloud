{
  "app": {
    "name": "DADI.Cloud API"
  },
  "auth": {
    "datastore": "@dadi/api-mongodb",
    "database": "cloud"
  },
  "caching": {
    "directory": {
      "enabled": false,
      "path": "./cache/api"
    },
    "redis": {
      "enabled": false
    }
  },
  "cors": true,
  "datastore": "@dadi/api-mongodb",
  "env": "qa",
  "media": {
    "basePath": "../web/workspace/public",
    "storage": "disk"
  },
  "paths": {
    "collections": "workspace/collections",
    "endpoints": "workspace/endpoints",
    "hooks": "workspace/hooks"
  },
  "publicUrl": {
    "host": "beta2.dadi.cloud",
    "port": 3001
  },
  "server": {
    "host": "0.0.0.0",
    "port": 3001,
    "protocol": "http"
  },
  "apidoc": {
    "title": "DADI Cloud API",
    "description": "This is the _Content API_ for [dadi.cloud](https://dadi.cloud).",
    "markdown": false,
    "path": "docs",
    "generateCodeSnippets": false,
    "themeVariables": "default",
    "themeTemplate": "triple",
    "themeStyle": "default",
    "themeCondenseNav": true,
    "themeFullWidth": false
  }
}
